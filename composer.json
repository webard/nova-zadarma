{
  "name": "webard/nova-zadarma",
  "description": "Zadarma VoIP integration to Laravel Nova. Make and receive phone calls directly from your Nova interface.",
  "keywords": [
    "laravel",
    "nova",
    "zadarma",
    "voip",
    "phone"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bartlomiej Gajda",
      "email": "code@webard.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^10.0|^11.0",
    "laravel/nova": "^4.20",
    "propaganistas/laravel-phone": "^5.0",
    "zadarma/user-api-v1": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Webard\\NovaZadarma\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webard\\NovaZadarma\\NovaZadarmaServiceProvider"
      ]
    }
  },
  "repositories": {
    "nova": {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.0"
  }
}