{
  "name": "webard/nova-zadarma",
  "description": "A Laravel Nova asset.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "*",
    "zadarma/user-api-v1": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Webard\\NovaZadarma\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webard\\NovaZadarma\\NovaZadarmaServiceProvider"
      ]
    }
  },
  "repositories": {
    "nova": {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.14"
  }
}
